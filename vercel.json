{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    },
    {
      "src": "/build",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/build/$1"
    }
  ]
}
